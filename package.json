{
  "name":    "netvest-backend",
  "version": "0.0.0",
  "private":  true,

  "main": "src/",

  "scripts":
  {
    "start": "node .",
    "dev":   "nodemon --watch src/ .",

    "st": "eslint -c node_modules/js-outlander/outlander.eslint.js src/ db/",

    "knex": "knex --knexfile=db/knexfile.js --env=dev",
    "db-migrate":  "npm run knex -- migrate:latest",
    "db-rollback": "npm run knex -- migrate:rollback"
  },

  "dependencies":
  {
    "express":
      "^4.13",

    "knex":
      "0.10",

    "pg":
      "^4.5",

    "fs-sync":
      "^1.0.2",

    "body-parser":
      "^1.15.0",

    "express-session":
      "^1.13.0",

    "passport":
      "^0.3.2",

    "passport-local":
      "^1.0.0",

    "promisify-node":
      "^0.1.5",

    "lodash":
      "^4",

    "console-ultimate":
      "^2.8",

    "rootpath":
      "StreetStrider/rootpath#v1.4.0"
  },

  "devDependencies":
  {
    "eslint":
      "^2.8",

    "js-outlander":
      "StrangeTransistor/js-outlander#2.0.0",

    "nodemon":
      "^1.9.1"
  }
}
